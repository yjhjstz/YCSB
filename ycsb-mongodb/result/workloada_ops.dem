set terminal png
set output 'workloada_ops.png'
set key inside right top vertical Right noreverse noenhanced autotitle nobox
set datafile missing '-'
set style data linespoints
set xtics border in scale 1,0.5 nomirror rotate by -45  autojustify
set xtics  norangelimit
set xtics   ()
set xlabel "connections"
set ylabel "ops/sec"
set title "workloada (50% read + 50% update)"
x = 0.0
i = 22
plot 'workloada_ops.dat' using 2:xtic(1) title columnheader(2), for [i=2:1] '' using i title columnheader(i)
